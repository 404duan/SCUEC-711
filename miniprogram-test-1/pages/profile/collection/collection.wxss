page
{
  background-color: #f9f9f9;
  background-position: center;
  background-repeat: no-repeat;
  background-image: url(data:image/png;base64,);
}

#fullpage
{
  width:100%;
  height:100vh;
  background-color: #f0f0f0;
}

.title
{
    width: 100%;
    height:80rpx;
    font-size: 40rpx;
    font-weight: bold;
    background-color: #f5f5f5;
    color:gray;
    padding-left: 20rpx;
    padding-right: 60rpx;
    text-align: right;
}

.goodsTag
{
    height:220rpx;
    width: 100%;
    background-color:white;
    margin-bottom: 5px;
    font-size: 33rpx;
    display: flex;
    flex-direction: row;
}

.buttonDel
{
    width:80rpx;
    height:100%;
    right:0;
    top:0;
    text-align: right;
    padding: 10rpx;
}
.buttonDel image
{
    width:40rpx;
    height:40rpx;
    margin-top: 90rpx;
    /* background-color:chocolate; */
}

.goodsArea
{
    padding-top: 10rpx;
    padding-bottom: 10rpx;
    height:100%;
    width:70%;
    display: flex;
    flex-direction: row;
}

.goodsImg
{
    height:100%;
    padding-top: 4%;
    padding-bottom: 4%;
    width:25%;
}

.goodsImg image
{
    object-fit: cover;
    height: 100%;
    width: 100%;
}

.textArea
{
    padding-top: 10rpx;
    padding-bottom: 10rpx;
    padding-left: 10rpx;
    height:100%;
    width:75%;
    flex-direction: column;
}

.buttonAdd
{
    height:100%;
    width:20%;
    right:0%;
    /* background-color:rgb(186, 221, 133); */
    color:white;
    text-align: center;
    font-size:40rpx;
    font-weight: bold;
    padding: 10rpx;
    padding-top:50rpx; 
}

.msg
{
  width:50%;
  height:auto;
  position: fixed;
  bottom: 25%;
  text-align: center;
  margin-left:25%;
  background-color:rgba(0,0,0,0.5);
  border-radius: 15rpx;
  color:white;
}